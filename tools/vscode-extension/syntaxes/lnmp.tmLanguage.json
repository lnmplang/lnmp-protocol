{
  "scopeName": "source.lnmp",
  "patterns": [
    {
      "name": "entity.name.tag.lnmp",
      "match": "F\\d+"
    },
    {
      "name": "keyword.operator.assignment.lnmp",
      "match": "="
    },
    {
      "name": "string.quoted.double.lnmp",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.lnmp",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "constant.numeric.lnmp",
      "match": "(?<==)[-+]?(?:0x[0-9A-Fa-f]+|\\d+)(?=;|$)"
    },
    {
      "name": "punctuation.separator.field.lnmp",
      "match": ";"
    }
  ],
  "repository": {},
  "fileTypes": ["lnmp"],
  "name": "LNMP"
}
